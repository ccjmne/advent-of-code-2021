{
  "name": "advent-of-code-2021",
  "version": "2021.12.1",
  "description": "My entries for https://adventofcode.com in 2021! ðŸ¥³",
  "main": "dist/run.js",
  "type": "module",
  "scripts": {
    "postinstall": "chmod u+x tools/prepare.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "once": "node --no-warnings --experimental-specifier-resolution=node --loader ./tools/loader.js run.ts",
    "dev": "tools/prepare.sh && set NODE_OPTIONS=--max-old-space-size=8192 && node --no-warnings --experimental-specifier-resolution=node --loader ./tools/loader.js run.ts --watch",
    "dev:full-rebuild": "tools/prepare.sh && nodemon --quiet --no-warnings --watch src --ext '*' --experimental-specifier-resolution=node --loader ./tools/loader.js run.ts",
    "runrun": "node --no-warnings --experimental-specifier-resolution=node --loader ./tools/loader.js run.ts --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ccjmne/advent-of-code-2021.git"
  },
  "keywords": [
    "AdventOfCode",
    "advent",
    "code"
  ],
  "author": "Eric NICOLAS <ccjmne@gmail.com> (ccjmne)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ccjmne/advent-of-code-2021/issues"
  },
  "homepage": "https://github.com/ccjmne/advent-of-code-2021#readme",
  "devDependencies": {
    "@colors/colors": "^1.6.0",
    "@types/node-dijkstra": "^2.5.6",
    "@types/nodemon": "^1.19.6",
    "@types/webpack": "^5.28.5",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "node-dijkstra": "^2.5.0",
    "nodemon": "^3.0.2",
    "raw-loader": "^4.0.2",
    "rxjs": "^7.8.1",
    "threads-esm": "^2.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "yargs": "^17.7.2"
  }
}
